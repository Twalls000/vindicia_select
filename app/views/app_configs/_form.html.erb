<%= form_for(@app_config) do |f| %>
  <%= render partial: 'layouts/form_errors', locals: {object: @app_config }%>

  <div class="field">
    <%= f.label :key %><br>
    <%= f.text_field :key %>
  </div>
  <div class="field">
    <%= f.label :value %><br>
    <%= f.text_area :value %>
  </div>
  <div class="actions">
    <%= f.submit class:'btn right' %>
  </div>
<% end %>
