<!DOCTYPE html>
<html>
<head>
  <title>VindiciaSelect</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <!--Import Google Icon Font-->
  <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <%= yield :head %>
</head>
<body>

  <nav class="darken-4" role="navigation">
  <div class="nav-wrapper container">
  <a class="brand-logo" href="<%= root_path %>" id="logo-container">
    Vindicia <span>Select</span>
  </a>
  <ul class="right hide-on-med-and-down">
  <li><%= link_to 'About', about_url %></li>
  <li><%= link_to 'Dashboard', dashboards_url %></li>
  <li><%= link_to 'Delayed Job', delayed_job_url %></li>
  </ul>
  </div>
  </nav>

  <div class="container"><%= yield %></div>
  <%= yield :javascript %>

</body>
</html>
